    "querystring-browser": "^1.0.4",
    "require-extension-hooks": "^0.3.3",
    "socket.io-client": "^2.2.0",
    "video.js": "^7.6.5",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
