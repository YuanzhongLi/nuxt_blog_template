    "dev": "cross-env NODE_ENV=development nuxt --hostname 0.0.0.0",
    "eslint": "eslint --cache --ext .js,.vue .",
    "eslint:fix": "npm run eslint -- --fix",
    "generate": "nuxt generate",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "prettier": "prettier --write src/**/*.js",
    "start": "nuxt start",
    "start:testbranch": "NUXT_PORT=9090 nuxt start",
    "stylelint": "stylelint --cache \"src/**/*.vue\" \"src/**/*.scss\" --config .stylelintrc.json",
    "stylelint:fix": "npm run stylelint -- --fix",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/device": "^1.2.3",
    "@nuxtjs/google-tag-manager": "^2.3.1",
    "@nuxtjs/moment": "^1.1.0",
    "@nuxtjs/sitemap": "^2.0.1",
    "@nuxtjs/style-resources": "^0.1.2",
    "aws-sdk": "^2.459.0",
    "axios": "^0.19.0",
    "browser-env": "^3.2.5",
    "consola": "^2.6.2",
    "cookieparser": "^0.1.0",
    "cross-env": "^5.2.0",
    "element-ui": "^2.8.2",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^2.2.0",
