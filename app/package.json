    "vue-infinite-loading": "^2.4.4",
    "vue-lazyload": "^1.2.6",
    "vue-multiselect": "^2.1.6",
    "vue-slider-component": "^3.1.0",
    "vue-test-utils": "^1.0.0-beta.11",
